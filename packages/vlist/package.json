{
  "name": "yav-list",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "StephenArk30/yav-list",
    "directory": "packages/vlist"
  },
  "homepage": "https://github.com/StephenArk30/yav-list#readme",
  "keywords": [],
  "main": "dist/yav-list.cjs",
  "module": "dist/yav-list.mjs",
  "directories": {
    "dist": "dist",
    "src": "src"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production && pnpm clean && rollup -c rollup.config.mjs",
    "dev": "pnpm clean && cross-env NODE_ENV=development && rollup -w -c rollup.dev.config.mjs",
    "prepublish": "pnpm clean",
    "publish": "pnpm build"
  },
  "dependencies": {
    "yav-list-common": "workspace:*",
    "yav-list-scroll": "workspace:*"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1"
  }
}
